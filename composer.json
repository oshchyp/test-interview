{
  "name": "adgoal/test-interview",
  "type": "service",
  "description": "Return statistics by ads programs",
  "license": [
    "proprietary"
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "ext-pdo": "*",
    "guzzlehttp/psr7": "^1.8",
    "league/container": "^3.3",
    "psr/container": "^2.0",
    "psr/http-message": "^1.0",
    "symfony/dotenv": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "require-dev": {
    "symfony/var-dumper": "^5.3"
  }
}
